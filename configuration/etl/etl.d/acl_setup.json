{
    "#": "Setup of Module Table",
    "defaults": {
        "acls": {
            "namespace": "ETL\\Maintenance",
            "options_class": "MaintenanceOptions",
            "destination": {
                "type": "mysql",
                "name": "Database",
                "config": "database",
                "schema": "moddb"
            }
        }
    },
    "acls": [
        {
            "name": "ACLTableManagement",
            "class": "ManageTables",
            "description": "Manage ACL related tables",
            "definition_file_list": [
                "modules.json"
            ],
            "enabled": true
        }
    ]
}
