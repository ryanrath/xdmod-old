{
  "defaults": {
    "global": {
      "endpoints": {
        "source": {
          "type": "jsonfile",
          "name": "ACL Source",
          "path": "acls/acls.json"
        },
        "destination": {
          "type": "mysql",
          "name": "Database",
          "config": "database",
          "schema": "moddb"
        }
      }
    }
  },

  "#": "Ingest a set of acls into the acls table",
  "acl-ingest": [
    {
      "name": "ACL JSON Import",
      "class": "StructuredFileIngestor",
      "namespace": "ETL\\Ingestor",
      "options_class": "IngestorOptions",
      "definition_file": "acls.json",
      "enabled": true,
      "truncate_destination": false,
      "description": "Import some acls"
    }
  ]
}
